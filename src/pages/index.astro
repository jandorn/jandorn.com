---
import Layout from "@/layouts/Layout.astro";
import Links from "@/components/Links.astro";
import Socials from "@/components/Socials.astro";
import "@/styles/global.css"
---

<Layout title="Jan Dorn">

  <!-- center content -->
	<div class="flex flex-col items-center justify-center h-full pb-[8em] space-y-6">

    <div class="relative">
      <img class="h-14" src="jandorn.svg" alt="jandorn.com logo">
    </div>
    <div class="flex justify-between w-full">
      <Links />
    </div>
    <Socials />
	</div>
</Layout>
